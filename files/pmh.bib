issn = {18775845},
journal = {Spat. Spatiotemporal. Epidemiol.},
keywords = {Built environment (BE),Activity space,Route length,be,built environment},
pages = {7},
publisher = {Elsevier Ltd},
title = {{Activity space metrics not associated with sociodemographic variables, diet or health outcomes in the Seattle Obesity Study II}},
