@article{Drewnowski2019c,
abstract = {Improving the built environment (BE) is viewed as one strategy to improve community diets and health. The present goal is to review the literature on the effects of BE on health, highlight its limitations, and explore the growing use of natural experiments in BE research, such as the advent of new supermarkets, revitalized parks, or new transportation systems. Based on recent studies on movers, a paradigm shift in built-environment health research may be imminent. Following the classic Moving to Opportunity study in the US, the present Moving to Health (M2H) strategy takes advantage of the fact that changing residential location can entail overnight changes in multiple BE variables. The necessary conditions for applying the M2H strategy to Geographic Information Systems (GIS) databases and to large longitudinal cohorts are outlined below. Also outlined are significant limitations of this approach, including the use of electronic medical records in lieu of survey data. The key research question is whether documented changes in BE exposure can be linked to changes in health outcomes in a causal manner. The use of geo-localized clinical information from regional health care systems should permit new insights into the social and environmental determinants of health.},
author = {Drewnowski, A. and Arterburn, David E and Zane, J. and Aggarwal, A. and Gupta, S. and Hurvitz, Philip M. and Moudon, Anne Vernez and Bobb, J. and Cook, A. and Lozano, P. and Rosenberg, D.},
doi = {10.1016/j.ssmph.2018.100345},
file = {:C$\backslash$:/Users/phurvitz/OneDrive/mendeley/Drewnowski et al/SSM - Population Health/2019/Drewnowski et al.{\_}2019{\_}The Moving to Health (M2H) approach to natural experiment research A paradigm shift for studies on built environm.pdf:pdf},
issn = {23528273},
journal = {SSM - Popul. Heal.},
keywords = {Built environment (BE),Diabetes,Electronic medical records,Geographic information systems (GIS),Natural experiments,Obesity,Residential mobility},
number = {December 2018},
pages = {100345},
publisher = {Elsevier},
title = {{The Moving to Health (M2H) approach to natural experiment research: A paradigm shift for studies on built environment and health}},
url = {https://doi.org/10.1016/j.ssmph.2018.100345},
volume = {7},
year = {2019}
}
